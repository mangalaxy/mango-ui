{
  "name": "mango-ui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.2",
    "formik": "^2.0.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.26.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropdown-select": "^3.7.0",
    "react-hot-loader": "^4.13.0",
    "react-markdown": "^5.0.3",
    "react-preloaders": "^3.0.3",
    "react-redux": "^5.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.2.0",
    "react-scroll": "^1.7.14",
    "react-select": "^3.0.8",
    "react-slick": "^0.25.2",
    "react-transition-group": "2.9.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0",
    "react-avatar-editor": "^11.0.9",
    "react-datepicker": "^3.0.0",
    "react-paginate": "^6.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "redux-devtools-extension": "^2.13.8"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "plugins": [
    "@babel/plugin-proposal-optional-chaining"
  ],

  "proxy": "http://mangostart-stage.us-east-2.elasticbeanstalk.com"
}
